<Window x:Class="DeltaSql.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeltaSql"
        xmlns:viewModels="clr-namespace:DeltaSql.ViewModels"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance {x:Type viewModels:MainWindowViewModel}}"
        Title="{Binding Translations.MainWindowTitle, FallbackValue=Delta SQL}" 
        Height="768" Width="1024" WindowStartupLocation="CenterScreen"
        Icon="pack://application:,,,/Images/Icon/DeltaSql.ico">
    <Grid Style="{StaticResource GridStyle}">

    </Grid>
</Window>
